ul,
ol
  margin 0
  padding 0
  list-style-position inside

ul
  list-style-type disc

ul ul,
ul ol,
ol ul,
ol ol
  margin-left 20px

b,
strong,
  font-weight bold

i,
em,
  font-style italic

ins
  text-decoration none

del
  text-decoration line-through

a,
button,
input,
textarea,
img
  outline 0

input,
button
  border-radius 0

img
  max-width 100%
  border none

button
  border none
  background none
